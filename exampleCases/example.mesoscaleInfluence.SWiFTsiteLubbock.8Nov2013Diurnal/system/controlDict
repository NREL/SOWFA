  stopAt            endTime;

  endTime           129600.0;

  deltaT            1.0;

  writeControl      adjustableRunTime;

  writeInterval     3600.0; 

  purgeWrite        0;

  writeFormat       binary;

  writePrecision    12;

  writeCompression  compressed;

  timeFormat        general;

  timePrecision     12;

  runTimeModifiable yes;

  adjustTimeStep    no;

  maxCo             0.75;

  maxDeltaT         25.0;

