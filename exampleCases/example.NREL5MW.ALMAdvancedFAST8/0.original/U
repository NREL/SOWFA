FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "../setUp"

 
dimensions      [0 1 -1 0 0 0 0];
 
internalField   uniform ($U0Mag 0 0);

boundaryField
{
    lower
    {
        type            cyclic;
    }
    upper
    {
        type            cyclic;
    }
    west
    {
        type            fixedValue;
        value           uniform ($U0Mag 0 0);
    }
    east
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           uniform ($U0Mag 0 0);
    }
    south
    {
        type            cyclic;
    }
    north
    {
        type            cyclic;
    }
}



