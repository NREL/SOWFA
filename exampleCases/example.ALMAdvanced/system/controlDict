  application       ABLSolver;

  libs              ("libSOWFAfiniteVolume.so" "libSOWFAincompressibleLESModels.so");

  startFrom         startTime;

  startTime         0.0;

  stopAt            endTime;

  endTime           16000.0;

  deltaT            0.5;

  writeControl      adjustableRunTime;

  writeInterval     1800.0; 

  purgeWrite        0;

  writeFormat       binary;

  writePrecision    12;

  writeCompression  compressed;

  timeFormat        general;

  timePrecision     12;

  runTimeModifiable yes;

  adjustTimeStep    yes;

  maxCo             0.75;

  maxDeltaT         25.0;

