FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "12000";
    object      pd;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -2 0 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    bottom
    {
        type            buoyantPressureMod;
        gradient        uniform 0;
        rho             rhok;
        value           uniform 0;
    }
    top
    {
        type            buoyantPressureMod;
        gradient        uniform 0;
        rho             rhok;
        value           uniform 0;
    }
    west 
    {
        type            cyclic;
    }
    east
    {
        type            cyclic;
    }
    north
    {
        type            cyclic;
    }
    south 
    {
        type            cyclic;
    }
}


