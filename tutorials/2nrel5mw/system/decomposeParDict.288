FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    note        "mesh decomposition control dictionary";
    location    "system";
    object      decomposeParDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains 288;
method          scotch;
preservePatches (north south east west);

simpleCoeffs
{
    n           (1 1 48);
    delta       0.001;
}

hierarchicalCoeffs
{
    n           (1 1 3);
    delta       0.001;
    order       xyz;
}

metisCoeffs
{
    processorWeights
    (
    );
}

scotchCoeffs
{
    //processorWeights
    //(
    //    1
    //    1
    //    1
    //    1
    //);
    //writeGraph  true;
    //strategy "b";
}

manualCoeffs
{
    dataFile    "decompositionData";
}


//// Is the case distributed
distributed     no;
