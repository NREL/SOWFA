FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      topoSetDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
 
actions
(
    {
       name      local;
       type      cellSet;
       action    new;
       source    cylinderToCell;
       sourceInfo
       {
          p1       (-378 0 0);   // start point on cylinder axis
          p2       (3024 0 0);   // end point on cylinder axis
          radius   126;
       }
    }
);
